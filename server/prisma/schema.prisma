// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

//criando uma tabela
model Habit {
  //@id diz que Ã© a PK
  id String @id @default(uuid())
  title String
  created_at DateTime

  //nome da tabela no banco
  @@map("habits")
}